                            <Button.Resources>
                                <Style TargetType="{x:Type Border}">
                                    <Setter Property="CornerRadius" Value="5"/>
                                </Style>
                            </Button.Resources>
                        </Button>
                    </Grid>
                </Border>
                <Button x:Name="startButton" Height="60" Width="170" Grid.Column="1" Grid.Row="3" Background="White" Click="startButton_Click">
                    <Button.Resources>
                        <Style TargetType="{x:Type Border}">
                            <Setter Property="CornerRadius" Value="5"/>
                        </Style>
                    </Button.Resources>
                    <TextBlock FontSize="30"  FontWeight="Bold">Start</TextBlock>
                </Button>
            </Grid>
        </TabItem>
        <TabItem x:Name="customExperienceTab" Visibility="Collapsed">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <Border Height="372" Width="780" Background="#464646" BorderBrush="#FEFEFE" BorderThickness="3" Grid.Column="1" Grid.Row="0" Grid.RowSpan="2" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,30">
                    <MediaElement x:Name="playVideo" LoadedBehavior="Manual" MediaEnded="playVideo_MediaEnded"></MediaElement>
                </Border>
                <Rectangle x:Name="exerciseInsturctionsBackground" Height="40" Width="720" Grid.Row="1" Fill="White" Opacity="0.60" Margin="0,65,0,0"/>
                <TextBlock Grid.Row="1" HorizontalAlignment="Center" Margin="0,115,0,0">
                    <Run x:Name="exerciseInsturctionsText" Foreground="Black" FontSize="25"/>
                </TextBlock>
                <Button x:Name="backToMainbutton" FontSize="17" Background="White" Height="30" Width="60" Grid.Column="0" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Bottom" Click="backToMainbutton_Click">
                    <StackPanel>
                        <Image Source="/Resources/Images &amp; Icons/Arrow.png" Height="20" />
                    </StackPanel>
                    <Button.Resources>
                        <Style TargetType="{x:Type Border}">
                            <Setter Property="CornerRadius" Value="5"/>
                        </Style>
                    </Button.Resources>
                </Button>
            </Grid>
        </TabItem>
    </TabControl>
</Window>
