                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Label x:Name="startTitleLabel" Content="Start" FontSize="30" Grid.Column="1" Grid.ColumnSpan="2" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,35"/>
                    <Border Background="White" BorderBrush="DarkGray" BorderThickness="0.7" Height="150" Width="430" Grid.Column="1" Grid.Row="0" Grid.RowSpan="3" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,25,0,0">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                            </Grid.RowDefinitions>
                            <TextBlock Grid.Column="0" Grid.Row="0" HorizontalAlignment="Center" VerticalAlignment="Center">
                                <TextBlock.Inlines>
                                    <Run FontWeight="Bold" FontSize="20" Text="Exercise Selected:" />
                                    <Run x:Name="exerciseSelectedTextblock" FontSize="20" Text="None" />
                                </TextBlock.Inlines>
                            </TextBlock>
                            <TextBlock Grid.Column="0" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center">
                                <TextBlock.Inlines>
                                    <Run FontWeight="Bold" FontSize="20" Text="Video Selected:" />
                                    <Run x:Name="videoSelectedTextblock" FontSize="20" Text="None" />
                                </TextBlock.Inlines>
                            </TextBlock>
                            <TextBlock Grid.Column="0" Grid.Row="2" HorizontalAlignment="Center" VerticalAlignment="Center">
                                <TextBlock.Inlines>
                                    <Run FontWeight="Bold" FontSize="20" Text="Audio Selected:" />
                                    <Run x:Name="audioSelectedTextblock" FontSize="20" Text="None" />
                                </TextBlock.Inlines>
                            </TextBlock>
                        </Grid>
                    </Border>
                    <Border Height="40" Width="400" Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="2" Margin="0,25,0,0">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition />
                            </Grid.RowDefinitions>
                            <Button x:Name="resetExerciseButton" Height="30" Width="120" Content="Reset Exercise" Background="White" Grid.Column="0" Click="resetExerciseButton_Click">
                                <Button.Resources>
                                    <Style TargetType="{x:Type Border}">
                                        <Setter Property="CornerRadius" Value="5"/>
                                    </Style>
                                </Button.Resources>
                            </Button>
                            <Button x:Name="resetVideoButton" Height="30" Width="120" Content="Reset Video" Background="White" Grid.Column="1" Click="resetVideoButton_Click">
                                <Button.Resources>
                                    <Style TargetType="{x:Type Border}">
                                        <Setter Property="CornerRadius" Value="5"/>
                                    </Style>
                                </Button.Resources>
                            </Button>
                            <Button x:Name="resetAudioButton" Height="30" Width="120" Content="Reset Audio" Background="White" Grid.Column="2" Click="resetAudioButton_Click">
                                <Button.Resources>
                                    <Style TargetType="{x:Type Border}">
                                        <Setter Property="CornerRadius" Value="5"/>
                                    </Style>
                                </Button.Resources>
                            </Button>
                        </Grid>
                    </Border>
                    <Button x:Name="startButton" Height="60" Width="170" Grid.Column="1" Grid.Row="3" Background="White" Click="startButton_Click">
                        <Button.Resources>
                            <Style TargetType="{x:Type Border}">
                                <Setter Property="CornerRadius" Value="5"/>
                            </Style>
                        </Button.Resources>
                        <TextBlock FontSize="30"  FontWeight="Bold">Start</TextBlock>
                    </Button>
                </Grid>
            </TabItem>
            <TabItem x:Name="customExperienceTab" Visibility="Collapsed">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Border MinHeight="395" MinWidth="825" Background="#464646" BorderBrush="#FEFEFE" BorderThickness="3" Grid.Column="1" Grid.Row="0" Grid.RowSpan="2" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,30">
                        <MediaElement x:Name="playVideo" LoadedBehavior="Manual" MediaEnded="playVideo_MediaEnded"></MediaElement>
                    </Border>
                    <Border x:Name="exerciseInsturctionsBackground" Height="40" Width="819" Grid.Row="1" Background="White" Opacity="0.60">
                        <TextBlock Grid.Row="1" HorizontalAlignment="Center" Margin="0,4,0,0">
                            <Run x:Name="exerciseInsturctionsText" Foreground="Black" FontSize="22"/>
                        </TextBlock>
                    </Border>
                    <Button x:Name="backToMainbutton" FontSize="17" Background="White" Height="30" Width="60" Grid.Column="0" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Bottom" Click="backToMainbutton_Click">
                        <StackPanel>
                            <Image Source="/Resources/Images &amp; Icons/Arrow.png" Height="20" />
                        </StackPanel>
                        <Button.Resources>
                            <Style TargetType="{x:Type Border}">
                                <Setter Property="CornerRadius" Value="5"/>
                            </Style>
                        </Button.Resources>
                    </Button>
                </Grid>
            </TabItem>
        </TabControl>
    </Viewbox>
</Window>
